dnorm(30,50,10)

##############

#Desvio padrÃ£o
d <- 47
#MÃ©dia da amostra
x <- 658
#Tamanho da amostra
n <- 100
#NÃ­vel de confianÃ§a
nc <- (1-0.90)/2
#Erro
error <- d/sqrt(n)
#Limite inferior
left <- x-(qnorm(nc, lower.tail = F)*error)
#Limite superior
right <- x+(qnorm(nc, lower.tail = F)*error)
cat("[",left, "-", right,"]")

####################

list <- c(4.37, 3.63, 2.78, 5.46, 2.18, 6.07, 3.24, 5.89, 4.86, 4.64)
d <- sd(list)
x <- mean(list)
n <- 10
nc <- (1-0.9)/2
#Erro
error <- d/sqrt(n)
#Limite inferior
left <- x-(qnorm(nc, lower.tail = F)*error)
#Limite superior
right <- x+(qnorm(nc, lower.tail = F)*error)
cat("[",left, "-", right,"]")
