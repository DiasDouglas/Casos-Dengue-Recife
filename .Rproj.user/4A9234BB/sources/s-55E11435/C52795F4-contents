sd <- 4250
erro <- 300
nc <- (1 - 0.9)/2
n <- ((qnorm(nc, lower.tail = F)*sd)/erro)^2
n

###################

sd <- 5
erro <- 1.5
nc <- (1 - 0.955)/2
n <- ((qnorm(nc, lower.tail = F)^2)*(sd^2)*1500)/
        ((erro^2)*(1500 - 1) + (qnorm(nc, lower.tail = F)^2)*(sd)^2)
n

###################

sd <- 5
erro <- 1
nc <- (1 - 0.98)/2
n <- ((qnorm(nc, lower.tail = F)*sd)/erro)^2
n

###################

sd <- 5
erro <- 1
nc <- (1 - 0.98)/2
n <- ((qnorm(nc, lower.tail = F)^2)*(sd^2)*5000)/
  ((erro^2)*(5000 - 1) + (qnorm(nc, lower.tail = F)^2)*(sd)^2)
n

